 #Local
 
docker build -t azure-file-mani-img:1.x .  
docker container run --name ptthr -p 443:443 azure-file-mani-img:1.x


#Local for Mac M1
  ##Init
docker buildx create --name mybuilder
docker buildx use mybuilder
docker buildx inspect --bootstrap
docker buildx ls

  ## build
docker buildx build --tag azure-file-mani-img:2.0 -o type=image --platform=linux/arm64,linux/amd64 .
docker buildx build --tag azure-file-mani-img:2.0 -o type=image --platform=linux/arm64 .
docker buildx build --tag azure-file-mani-img:2.0 -o type=image --platform=linux/amd64 .
docker buildx build --tag azure-file-mani-img:1.0-linux64 --output "type=docker,name=azure-file-mani-img:1.0-linux64" --platform=linux/arm64,linux/amd64 .



docker container run --name ptthr -p 443:443 azure-file-mani-img:1.0-linux64


#Remote

  ##Init
az login
az acr login --name HRWebservicesDEV
docker login hrwebservicesdev.azurecr.io -u HRWebservicesDEV -p YDfU4dgWtYz27CIG=izgICjVk5WYwbLK

  ## push
docker tag azure-file-mani-img:1.x hrwebservicesdev.azurecr.io/azure-file-mani-img:1.x
docker image push hrwebservicesdev.azurecr.io/azure-file-mani-img:1.x

